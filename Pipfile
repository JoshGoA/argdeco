[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[scripts]
build = "python setup.py sdist bdist_wheel"
upload_testpypi = "python -m twine upload --repository testpypi dist/*"
upload_pypi = "python -m twine upload dist/*"

[dev-packages]
autopep8 = "*"
pylint = "*"
twine = "*"

[packages]
wrapt = "*"

[requires]
python_version = "3.8"
